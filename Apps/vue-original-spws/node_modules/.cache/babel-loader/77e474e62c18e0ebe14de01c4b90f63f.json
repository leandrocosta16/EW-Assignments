AGA,mBAAA,MAAA,GAAA,QAAA,CAAA,IAAA;AAHA;AAAA;;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,KAbA;AAuBA,EAAA,OAAA,EAAA;AACA,IAAA,UAAA,EAAA,oBAAA,IAAA,EAAA;AACA,MAAA,KAAA,CAAA,uBAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAAA,CAAA;AACA;AAHA;AAvBA,CAAA","sourcesContent":["<template>\n  <v-card class=\"ma-2\">\n    <v-card-title>Lista dos Filmes na BD</v-card-title>\n    <v-card-text>\n      <v-data-table\n        :headers=\"hfilmes\"\n        :items=\"filmes\">\n\n          <template v-slot:no-data>\n            <v-alert :value=\"true\" color=\"warning\" icon=\"warning\">\n              Ainda não foi possível apresentar uma lista dos filmes...\n            </v-alert>\n          </template>\n\n          <v-template v-slot:items=\"props\">\n            <tr @click=\"rowClicked(props.item)\">\n              <td>{{ props.item.titulo }}</td>\n              <td>{{ props.item.data }}</td>\n              <td>{{ props.item.lingua }}</td>\n              <td>{{ props.item.duracao }}</td>\n            </tr>\n          </v-template>\n      </v-data-table>\n    </v-card-text>\n  </v-card>\n</template>\n\n<script>\nimport axios from 'axios'\nconst lhost = require(\"@/config/global\").host;\n\nexport default {\n  name: 'ListaFilmes',\n\n  data: () => ({\n    hfilmes: [\n      {text: \"Título\", sortable: true, value: 'titulo', class: 'subtitle-1'},\n      {text: \"Data\", sortable: true, value: 'data', class: 'subtitle-1'},\n      {text: \"Língua\", sortable: true, value: 'lingua', class: 'subtitle-1'},\n      {text: \"Duração\", sortable: true, value: 'duracao', class: 'subtitle-1'},\n    ],\n    filmes: []\n  }),\n\n  created: async function(){\n    try {\n      let response = await axios.get(lhost + \"/filmes\");\n      this.filmes = response.data\n    } \n    catch (e) {\n      return e;\n    }\n  },\n\n  methods: {\n    rowClicked: function(item){\n      alert('Cliquei no filme: ' + JSON.stringify(item))\n    }\n  }\n  \n}\n</script>\n"],"sourceRoot":"src/components"}]}